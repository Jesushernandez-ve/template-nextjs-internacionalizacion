{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"

    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "argsIgnorePattern": "^_"
    },
    "no-console": [
        2,
        {
        "allow": ["warn", "error"]
        }
    ]
}
